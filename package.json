{
 "name": "mqtt-test",
 "version": "1.0.0",
 "main": "index.js",
 "license": "MIT",
 "scripts": {
  "start": "ts-node-dev --transpile-only --respawn --ignore-watch node_modules src/server.ts",
  "test": "jest --passWithNoTests --noStackTrace --runInBand",
  "test:cov": "yarn test --coverage"
 },
 "author": "Giovanne Brito Barbosa",
 "dependencies": {
  "axios": "^0.21.1",
  "body-parser": "^1.19.0",
  "bull": "^4.7.0",
  "cors": "^2.8.5",
  "dotenv": "^16.0.0",
  "express": "^4.17.3",
  "ts-jest": "^27.0.5",
  "uuid-random": "^1.3.2",
  "winston": "^3.3.3"
 },
 "devDependencies": {
  "@types/bull": "^3.15.8",
  "@types/cors": "^2.8.7",
  "@types/express": "^4.17.7",
  "@types/jest": "^27.0.1",
  "jest": "^27.0.6",
  "supertest": "^6.1.6",
  "ts-node-dev": "^1.0.0-pre.61",
  "typescript": "^4.0.2"
 },
 "jest": {
  "moduleFileExtensions": [
   "js",
   "json",
   "ts"
  ],
  "rootDir": ".",
  "testRegex": ".*\\.spec\\.ts$",
  "transform": {
   "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": [
   "<rootDir>/src/services/*.ts",
   "!**/index.ts"
  ],
  "coverageDirectory": "coverage",
  "testEnvironment": "node"
 }
}
